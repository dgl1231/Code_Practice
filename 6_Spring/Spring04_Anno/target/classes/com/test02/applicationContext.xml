<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
		http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context-4.3.xsd">


	<context:annotation-config />
	<bean id="lee" class="com.test02.Student">
		<property name="name" value="이동건"></property>
		<property name="addr" value="서울시 강동구"></property>
		<property name="age" value="26"></property>
	</bean>
	<bean id="kim" class="com.test02.Student">
		<property name="name" value="양윤열"></property>
		<property name="addr" value="수원시 "></property>
		<property name="age" value="26"></property>
	</bean>
	
	<bean id="mySchool" class="com.test02.School">
		<property name="grade" value="1"></property>
	</bean>
	<bean id="mySchool2" class="com.test02.School">
		<property name="grade" value="2"></property>
	</bean>
</beans>
